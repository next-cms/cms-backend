[
    {
        "name": "Button",
        "vendor": "antd",
        "importSignature": "import {Button} from \"antd\";",
        "props": {
            "disabled": {
                "type": "boolean",
                "isRequired": false,
                "default": false,
                "description": "disabled state of button"
            },
            "ghost": {
                "type": "boolean",
                "isRequired": false,
                "default": false,
                "description": "make background transparent and invert text and border colors, added in 2.7"
            },
            "href": {
                "type": "string",
                "isRequired": false,
                "default": "",
                "description": "redirect url of link button"
            },
            "htmlType": {
                "type": "select",
                "options": ["submit", "reset", "button"],
                "isRequired": false,
                "default": "button",
                "description": "set the original html <code>type</code> of <code>button</code>, see: <a href=\"https://developer.mozilla.org/en-US/docs/Web/HTML/Element/button#attr-type\">MDN</a>"
            },
            "icon": {
                "type": "string",
                "isRequired": false,
                "description": "set the icon of button, see: Icon component"
            },
            "loading": {
                "type": "boolean|object",
                "syntax": {"delay": "number"},
                "default": false,
                "isRequired": false,
                "description": "set the icon of button, see: Icon component"
            },
            "shape": {
                "type": "select",
                "options": ["circle", "round", "default"],
                "default": "default",
                "isRequired": false,
                "description": "Can be set to <code>circle</code>, <code>round</code> or omitted"
            },
            "size": {
                "type": "select",
                "options": ["small", "large", "default"],
                "default": "default",
                "isRequired": false,
                "description": "Can be set to <code>small</code> <code>large</code> or omitted"
            },
            "target": {
                "type": "string",
                "isRequired": false,
                "description": "same as target attribute of a, works when href is specified"
            },
            "type": {
                "type": "select",
                "options": ["primary", "ghost", "dashed", "danger", "link", "default"],
                "default": "default",
                "isRequired": false,
                "description": "Can be set to <code>primary</code> <code>ghost</code> <code>dashed</code> <code>danger</code> <code>link</code>(added in 3.17) or omitted (meaning <code>default</code>)"
            },
            "onClick": {
                "type": "function",
                "syntax": "(event)=>{}",
                "isRequired": false,
                "description": "Set the handler to handle <code>click</code> event"
            },
            "block": {
                "type": "boolean",
                "default": false,
                "isRequired": false,
                "description": "option to fit button width to its parent width"
            },
            "style": {
                "type": "object",
                "isRequired": false
            },
            "className": {
                "type": "string",
                "isRequired": false
            }
        }
    },
