    {
        "name": "Icon",
        "vendor": "antd",
        "importSignature": "import {Icon} from \"antd\";",
        "props": {
            "type": {
                "type": "string",
                "isRequired": true,
                "description": "Type of the ant design icon. See <a href=\"https://ant.design/components/icon/\">here</a> for the available types."
            },
            "style": {
                "type": "object",
                "isRequired": false,
                "description": "Style properties of icon, like <code>fontSize</code> and <code>color</code>"
            },
            "theme": {
                "type": "select",
                "options": ["filled", "outlined", "towTone"],
                "default": "outlined",
                "isRequired": false,
                "description": "Style properties of icon, like <code>fontSize</code> and <code>color</code>"
            },
            "spin": {
                "type": "boolean",
                "default": false,
                "isRequired": false,
                "description": "Rotate icon with animation"
            },
            "rotate": {
                "type": "number",
                "isRequired": false,
                "description": "Rotate degrees (added in 3.13.0, not working in IE9)"
            },
            "component": {
                "type": "element",
                "isRequired": false,
                "description": "The component used for the root node. This will override the <code>type</code> property. See <a href=\"https://ant.design/components/icon/#API\">here</a> for details of the components."
            },
            "twoToneColor": {
                "type": "string",
                "isRequired": false,
                "description": "Only supports the two-tone icon. Specify the primary color."
            }
        }
    },
